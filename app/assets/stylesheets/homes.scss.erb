.container-fluid{
  margin :0px auto;
  text-align:center;
}

.port-bg{
  background-color: #ECEFF1;
}

.port-massage{
  margin-bottom:30px;
}

.portfolio-box{
  margin-top:90px;
  text-align:center;
}

.port-border{
  border-bottom: #EC715F 3px solid;
  width:70px;
  margin-top:8px;
  margin-bottom:30px;
}

.port-box{
  width:920px;
}

.port-boxes{
  width:300px;
  height:250px;
  background-size: cover;
  background-position: center;
  margin-right: 10px;
  margin-bottom: 10px;
}

.port-boxes:nth-child(1){
  background-image: url(<%= asset_path "sketch.jpg" %>);
}

.port-boxes:nth-child(2){
  background-image: url(<%= asset_path "websuku.png" %>);
}

.port-boxes:nth-child(3){
  background-image: url(<%= asset_path "write.jpg" %>);
  margin-right: 0px;
}

.port-boxes:nth-child(4){
  background-image: url(<%= asset_path "webcamp.png" %>);
}

.port-boxes:nth-child(5){
  background-image: url(<%= asset_path "talk.jpg" %>);
}

.port-boxes:nth-child(6){
  background-image: url(<%= asset_path "webcamp-mother.png" %>);
  margin-right: 0px;
}