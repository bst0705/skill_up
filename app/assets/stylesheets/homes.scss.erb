.container-fluid{
  margin :0px auto;
  text-align:center;
}

.port-bg{
  background-color: #ECEFF1;
  padding:90px 0px;
}

.port-massage{
  margin-bottom:30px;
}

.portfolio-box{
  text-align:center;
}

.port-border{
  border-bottom: #EC715F 3px solid;
  width:70px;
  margin-top:8px;
  margin-bottom:30px;
}

.port-box{
  width:90%;
}

.port-boxs{
  width:90%;
}

.port-boxes{

  height:250px;
  background-size: cover;
  background-position: center;
  margin-right: 10px;
  margin-bottom: 10px;
}

.port-boxes:nth-child(1){
  background-image: url(<%= asset_path "sketch.jpg" %>);
}

.port-boxes:nth-child(2){
  background-image: url(<%= asset_path "websuku.png" %>);
}

.port-boxes:nth-child(3){
  background-image: url(<%= asset_path "write.jpg" %>);
}

.port-boxess{

  height:250px;
  background-size: cover;
  background-position: center;
  margin-right: 10px;
  margin-bottom: 10px;
}

.port-boxess:nth-child(1){
  background-image: url(<%= asset_path "webcamp.png" %>);
}

.port-boxess:nth-child(2){
  background-image: url(<%= asset_path "talk.jpg" %>);
}

.port-boxess:nth-child(3){
  background-image: url(<%= asset_path "webcamp-mother.png" %>);
}

.container-Feature{
  width:980px;
  margin:90px auto;
}

.Featurebox{
  margin-bottom:30px;
}

.strength-boxes{
  width:30%;
  margin-right:10px;
}

.strength-boxes:nth-child(3){
  margin-right:0px;
}

.profile-box{
  width: 1024px;
}

.profile-image-box{
  width: 512px;
}

.profile-image{
  width: 250px;
  height: 250px;
  background-image: url(<%= asset_path "sketch.jpg" %>);
  background-size: cover;
  background-position: center;
}

.profile-massage-box{
  width: 90%;
}

.contact-container{
  margin:90px auto;
  width:800px;
}

.contact-render-box{
  width: 400px;
}

.contact-render-boxes{
  width: 150px;
}

.contact{
  width: 400px;
}

.contact-sns-box{
  width:210px;
}

.sns{
  font-size:20px;
}

.row{
  margin: 0px;
}

.sns-item{
  width: 50px;
  height: 50px;
  margin-right: 20px;
}
.sns-item:nth-child(3){
  margin-right: 0px;
}

.massage{
  text-align:left;
}


@media screen and (max-width: 768px){
.massage {
    text-align: center;
}
}

